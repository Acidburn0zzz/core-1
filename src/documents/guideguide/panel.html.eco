<!DOCTYPE HTML>
<html>
<head>
  <title>GuideGuide</title>

  <link rel="stylesheet" href="style.css" type="text/css">

  <script type="text/javascript" src="jquery-1.10.2.js"></script>
  <script type="text/javascript" src="script.js"></script>
</head>
<body class="guideguide-dark" data-environment="development">
  <div id="guideguide" data-theme="guideguide-dark">

    <div class="panel-forms js-tabbed-pages height-stretch">

      <div class="nav">
        <div class="left">
          <a class="nav-item js-tabbed-page-tab js-grid-tab is-selected" data-page="form" data-localize="ui.grid" href>Grid</a>
          <a class="nav-item js-tabbed-page-tab js-custom-tab" data-page="custom" data-localize="ui.custom" href>Custom</a>
          <a class="nav-item js-tabbed-page-tab js-sets-tab" data-page="sets" data-localize="ui.sets" href>Sets</a>
          <a class="nav-item js-tabbed-page-tab js-settings-tab" data-page="settings" href><span class="icon icon-settings"></span></a>
        </div>
        <div class="right">
          <span class="update-indicator">
            <a class="header-action js-has-update-button" href><span class="icon icon-arrow-up"></span></a>
          </span>
          <a class="header-action js-toggle-guide-visibility" href><span class="icon icon-visibility"></span></a>
        </div>
      </div> <!-- end .nav -->

      <div class="pages height-stretch">
        <div class="tabbed-page js-tabbed-page js-grid-form is-selected" data-page="form">
          <div class="height-stretch stretch-form">
            <table class="grid-form">
              <tr class="set-name-row js-set-name-row">
                <td colspan="2">
                  <span class="input js-input">
                    <span class="input-shell">
                      <input type="text" spellcheck="false" class="set-name js-set-name " data-type="setName" title="Set name" placeholder="Set name">
                    </span>
                  </span>
                </td>
              </tr>
              <tr>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="margin">
                    <span class="input-shell">
                      <a class="icon icon-margin-left js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="marginLeft" title="Left margin">
                    </span>
                  </span>
                </td>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="margin">
                    <span class="input-shell">
                      <a class="icon icon-margin-top js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="marginTop" title="Top margin">
                    </span>
                  </span>
                </td>
              </tr>
              <tr>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="margin">
                    <span class="input-shell">
                      <a class="icon icon-margin-right js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="marginRight" title="Right margin">
                    </span>
                  </span>
                </td>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="margin">
                    <span class="input-shell">
                      <a class="icon icon-margin-bottom js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="marginBottom" title="Bottom margin">
                    </span>
                  </span>
                </td>
              </tr>
              <tr>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="count">
                    <span class="input-shell">
                      <a class="icon icon-count-column js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="countColumn" title="Column count" data-integer="true">
                    </span>
                  </span>
                </td>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="count">
                    <span class="input-shell">
                      <a class="icon icon-count-row js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="countRow" title="Row count" data-integer="true">
                    </span>
                  </span>
                </td>
              </tr>
              <tr>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="width">
                    <span class="input-shell">
                      <a class="icon icon-width-column js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="widthColumn" title="Column width">
                    </span>
                  </span>
                </td>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="width">
                    <span class="input-shell">
                      <a class="icon icon-width-row js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="widthRow" title="Row width">
                    </span>
                  </span>
                </td>
              </tr>
              <tr>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="gutter">
                    <span class="input-shell">
                      <a class="icon icon-width-column-gutter js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="gutterColumn" title="Column gutter">
                    </span>
                  </span>
                </td>
                <td>
                  <span class="input iconned js-input js-grid-form-iconned-input" data-distribute="gutter">
                    <span class="input-shell">
                      <a class="icon icon-width-row-gutter js-iconned-input-button" tabindex="-1" href></a>
                      <input type="text" spellcheck="false" class="js-grid-form-input " data-type="gutterRow" title="Row gutter">
                    </span>
                  </span>
                </td>
              </tr>
            </table>
          </div>
          <div class="form-action-buttons">
            <a class="button primary js-make-grid make-grid" data-localize="ui.btnMakeGrid" href>Make grid</a>
            <a class="button primary js-save-set save-set" href>Save set</a>
            <a class="button icon-button js-new-set add-set" href><span class="icon icon-set-add"></span></a>
            <a class="button icon-button js-cancel-set cancel-set" href><span class="icon icon-clear-guides"></span></a>
          </div>
        </div> <!-- end .tabbed-page -->

        <div class="ggn-form tabbed-page js-tabbed-page js-custom-form" data-page="custom">
          <table class="form-table">
            <tr class="form-wrapper">
              <td>
                <table class="grid-form">
                  <tr class="set-name-row js-set-name-row">
                    <td colspan="2">
                      <span class="input js-input">
                        <span class="input-shell">
                          <input type="text" spellcheck="false" class="set-name js-set-name" data-type="setName" title="Set name" placeholder="Set name">
                          <input type="hidden" class="js-set-id">
                        </span>
                      </span>
                    </td>
                  </tr>
                  <tr>
                    <td>
                      <span class="input height-fill js-input">
                        <span class="input-shell">
                          <textarea class="js-custom-input" spellcheck="false"></textarea>
                        </span>
                      </span>
                    </td>
                  </tr>
                </table>
              </td>
            </tr>
            <tr class="form-actions">
              <td>
                <div class="form-action-buttons">
                  <a class="button primary js-make-grid make-grid" data-localize="ui.btnMakeGrid" href>Make grid</a>
                  <a class="button primary js-save-set save-set" href>Save set</a>
                  <a class="button icon-button js-new-set add-set" href><span class="icon icon-set-add"></span></a>
                  <a class="button icon-button js-cancel-set cancel-set" href><span class="icon icon-clear-guides"></span></a>
                </div>
              </td>
            </tr>
          </table>
        </div> <!-- end .tabbed-page -->

        <div class="tabbed-page js-tabbed-page js-sets-form" data-page="sets">
          <table class="form-table">
            <tr class="form-wrapper">
              <td>
                <ul class="set-list js-set-list">
                </ul>
                <!-- template for set list -->
                <ul class="hidden-template">
                  <li class="js-set-item-template js-set set-list-set">
                    <a class="js-set-select" href>
                      <span class="js-set-item-name">Foo</span>
                    </a>
                    <a class="js-edit-set icon icon-edit set-action" href></a>
                    <a class="js-delete-set icon icon-clear-guides set-action" href></a>
                  </li>
                </ul>
                <!-- end template for set list -->
              </td>
            </tr>
            <tr class="form-actions">
              <td>
                <div class="form-action-buttons">
                  <a class="button primary js-make-grid" data-localize="ui.btnMakeGrid" href>Make grid</a>
                  <a class="button icon-button js-new-set" href><span class="icon icon-set-add"></span></a>
                </div>
              </td>
            </tr>
          </table>
        </div> <!-- end .tabbed-page -->

        <div class="tabbed-page js-tabbed-page js-settings-form" data-page="settings">
          <table class="form-table">
            <tr class="form-wrapper">
              <td>
                <div class="settings-list js-settings-list">
              <div class="settings-inner">

                <div class="setting">
                  <h3 data-localize="ui.updates">Updates</h3>
                  <ul class="button-group">
                    <li><a class="button js-check-for-updates" data-localize="ui.btnCheckForUpdates" href>Check for updates</a></li>
                  </ul>
                </div>

                <div class="setting">
                  <h3 data-localize="ui.sets">Sets</h3>
                  <ul class="button-group">
                    <li><a class="button js-import-sets" data-localize="ui.btnImport" href>Import</a></li>
                    <li><a class="button js-export-sets" data-localize="ui.btnExport" href>Export</a></li>
                  </ul>
                </div>

                <div class="setting js-setting">
                  <h3 data-localize="ui.titleHorizontalPosition">Horizontal position</h3>
                  <div class="dropdown js-dropdown" data-setting="horizontalPosition">
                    <a class="dropdown-button js-dropdown-button" href>Left</a>
                    <ul class="dropdown-list js-dropdown-list">
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingHorizontalFirst" data-value="first" href>Left</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingHorizontalCenter" data-value="center" href>Center</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingHorizontalLast" data-value="last" href>Right</a></li>
                    </ul>
                  </div>
                </div>

                <div class="setting js-setting">
                  <h3 data-localize="ui.titleVerticalPosition">Vertical position</h3>
                  <div class="dropdown js-dropdown" data-setting="verticalPosition">
                    <a class="dropdown-button js-dropdown-button" href>Top</a>
                    <ul class="dropdown-list js-dropdown-list">
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingVerticalFirst" data-value="first" href>Top</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingVerticalCenter" data-value="center" href>Center</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingVerticalLast" data-value="last" href>Bottom</a></li>
                    </ul>
                  </div>
                </div>

                <div class="setting js-setting">
                  <h3 data-localize="ui.titleHorizontalRemainder">Horizontal remainder</h3>
                  <div class="dropdown js-dropdown" data-setting="horizontalRemainder">
                    <a class="dropdown-button js-dropdown-button" href>Left</a>
                    <ul class="dropdown-list js-dropdown-list">
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingHorizontalFirst" data-value="first" href>Top</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingHorizontalCenter" data-value="center" href>Center</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingHorizontalLast" data-value="last" href>Last</a></li>
                    </ul>
                  </div>
                </div>

                <div class="setting js-setting">
                  <h3 data-localize="ui.titleVerticalRemainder">Vertical remainder</h3>
                  <div class="dropdown js-dropdown" data-setting="verticalRemainder">
                    <a class="dropdown-button js-dropdown-button" href>Top</a>
                    <ul class="dropdown-list js-dropdown-list">
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingVerticalFirst" data-value="first" href>First</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingVerticalCenter" data-value="center" href>Center</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.settingVerticalLast" data-value="last" href>Last</a></li>
                    </ul>
                  </div>
                </div>

                <div class="setting js-setting">
                  <h3 data-localization="ui.titleReportAnonymousData">Report anonymous data</h3>
                  <div class="dropdown js-dropdown" data-setting="reportAnonymousData">
                    <a class="dropdown-button js-dropdown-button" href>No</a>
                    <ul class="dropdown-list js-dropdown-list">
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.yes" data-value="true" href>Yes</a></li>
                      <li><a class="dropdown-item js-dropdown-item" data-localize="ui.no" data-value="false" href>No</a></li>
                    </ul>
                  </div>
                </div>

                </div>
              </div>
              </td>
            </tr>
            <tr class="form-actions">
              <td>
                <div class="form-action-buttons">
                  <a class="button primary js-save-settings" data-localize="ui.btnSaveSettings" href>Save settings</a>
                </div>
              </td>
            </tr>
          </table>
        </div> <!-- end .tabbed-page -->
      </div>
    </div>

    <div class="action-bar js-action-bar">
      <a class="button js-left" tabindex="-1" href><span class="icon icon-quick-left"></span></a>
      <a class="button js-vertical-midpoint" tabindex="-1" href><span class="icon icon-quick-vertical-midpoint"></span></a>
      <a class="button js-right" tabindex="-1" href><span class="icon icon-quick-right"></span></a>
      <a class="button button-clear-guides js-clear" tabindex="-1" href><span class="icon icon-clear-guides"></span></a>
      <a class="button js-top" tabindex="-1" href><span class="icon icon-quick-top"></span></a>
      <a class="button js-horizontal-midpoint" tabindex="-1" href><span class="icon icon-quick-horizontal-midpoint"></span></a>
      <a class="button js-bottom" tabindex="-1" href><span class="icon icon-quick-bottom"></span></a>
    </div> <!-- end .action-bar -->

    <div class="alert">
      <div class="alert-body js-alert-body">
        <div class="alert-content">
          <h3 class="alert-title js-alert-title"></h3>
          <div class="js-alert-message"></div>
        </div>
        <ul class="button-group js-alert-actions">
          <li><a class="button" href>Button</a></li>
        </ul>
      </div>
      <!-- alerts will go here when needed -->
    </div>

    <!-- templates for alerts -->
    <div class="hidden-template">
      <li class="js-button-template"><a class="button" href>Button</a></li>
    </div>
    <!-- end templates for alerts -->

    <div class="dropdown-backdrop js-dropdown-backdrop"></div>

  </div>
</body>
</html>
