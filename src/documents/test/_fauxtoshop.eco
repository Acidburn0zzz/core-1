module "Fauxtoshop"

test "can get OS", ->
  ok Fauxtoshop.getOS()

test "can get data", ->
  equal $.type(Fauxtoshop.getData()), "object"

test "can set data", ->
  equal $.type(Fauxtoshop.setData({foo: "bar"})), "object"

test "can toggle guides", ->
  equal Fauxtoshop.toggleGuides(), true

test "can get document info", ->
  propEqual Fauxtoshop.getDocumentInfo(), { hasOpenDocuments: false }

test "can add guide", ->
  guide =
    location: 10
    orientation: "horizontal"
  equal Fauxtoshop.addGuide(guide), guide
