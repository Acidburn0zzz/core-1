module "Core"

args =
  bridge: new GuideGuideHTMLBridge()
  locale: "en_us"

gg = new GuideGuideCore(args)

asyncTest "can init", ->
  expect 1

  new GuideGuideCore args, (err) ->
    ok !err, err
    start()

test "can create button object", ->
  expect 1
  propEqual gg.button("Ok","foo",true), {label:"Ok", callback: "foo", primary: true}
